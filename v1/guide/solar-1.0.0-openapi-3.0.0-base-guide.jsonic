# Guide

guide: {

  entity: moon: {
    path: '/api/planet/{planet_id}/moon': {  # ent:cmp=moon;path=t/p/t/;cr=1.500;path-primary
      op: create: method: *"POST"|string  # not-load
      op: list: method: *"GET"|string  # array
    }
    path: '/api/planet/{planet_id}/moon/{moon_id}': {  # ent:cmp=moon;path=t/p/;cr=1.500;path-primary
      rename: param: "moon_id": *"id"|string  # end-id
      op: load: method: *"GET"|string  # end-param
      op: remove: method: *"DELETE"|string  # not-load
      op: update: method: *"PUT"|string  # not-load
    }
  }

  entity: planet: {
    path: '/api/planet': {  # ent:cmp=planet;path=t/;cr=1.500;path-primary
      op: create: method: *"POST"|string  # not-load
      op: list: method: *"GET"|string  # array
    }
    path: '/api/planet/{planet_id}': {  # ent:cmp=planet;path=t/p/;cr=1.500;path-primary
      rename: param: "planet_id": *"id"|string  # end-id
      op: load: method: *"GET"|string  # end-param
      op: remove: method: *"DELETE"|string  # not-load
      op: update: method: *"PUT"|string  # not-load
    }
  }

}

