{
  "name": "@voxgig/apidef-validate-v1",
  "version": "0.0.1",
  "main": "dist/main.js",
  "type": "commonjs",
  "types": "dist/main.d.ts",
  "description": "Validate v1",
  "homepage": "https://github.com/voxgig/apidef-validate",
  "keywords": [
    "voxgig",
    "apidef"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxgig/apidef-validate.git"
  },
  "scripts": {
    "test": "node --enable-source-maps --test \"dist-test/**/*.test.js\"",
    "test-some": "node --enable-source-maps --test-name-pattern=\"$npm_config_pattern\" --test \"dist-test/**/*.test.js\"",
    "test-guide-case": "node --enable-source-maps --test-name-pattern=\"guide-case\" --test \"dist-test/**/*.test.js\"",
    "test-model-case": "node --enable-source-maps --test-name-pattern=\"model-case\" --test \"dist-test/**/*.test.js\"",
    "watch": "tsc --build src test -w",
    "build": "tsc --build src test",
    "clean": "rm -rf dist dist-test node_modules yarn.lock package-lock.json",
    "reset": "npm run clean && npm i && npm run build && npm test",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;"
  },
  "license": "MIT",
  "devDependencies": {
    "@hapi/code": "^9.0.3",
    "@types/node": "24.3.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@voxgig/apidef": "^2.4.1"
  }
}
